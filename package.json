{
  "name": "autoprefixer-transform",
  "version": "0.2.0",
  "description": "Autoprefixer transform stream",
  "bin": {
    "autoprefix": "./bin/autoprefix"
  },
  "main": "dist/transform.js",
  "scripts": {
    "test": "tape ./spec/*.js",
    "build": "rm -rf dist && babel lib --out-dir dist",
    "watch": "babel --watch lib --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/flawless/autoprefixer-transform.git"
  },
  "keywords": [
    "autoprefixer",
    "css",
    "transform",
    "pipe",
    "pipeline"
  ],
  "author": "Matt Styles",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/flawless/autoprefixer-transform/issues"
  },
  "homepage": "https://github.com/flawless/autoprefixer-transform",
  "dependencies": {
    "autoprefixer-core": "^5.1.9",
    "core-js": "^0.8.3",
    "minimist": "^1.1.1"
  },
  "devDependencies": {
    "babel": "^5.1.8",
    "osenv": "^0.1.0",
    "tape": "^4.0.0"
  }
}
